  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.7",
    "bootstrap": "^5.3.3",
    "env-cmd": "^10.1.0",
    "jest": "^27.5.1",
    "jest-sonar-reporter": "^2.0.0",
    "jquery": "^3.7.1",
    "jwt-decode": "^4.0.0",
